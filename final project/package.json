{
  "name": "final-project",
  "version": "1.0.0",
  "description": "Automated tests",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha --require ts-node/register tests/**/*.test.ts"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.5.5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "@typescript-eslint/no-unused-vars": "warn"
    }
  }
}
